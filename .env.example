# AWS Configuration
AWS_REGION=ap-northeast-2
AWS_PROFILE=aiops-dev

# AgentCore Configuration
AGENTCORE_ENDPOINT=https://agentcore.ap-northeast-2.amazonaws.com
AGENTCORE_RUNTIME_ROLE_ARN=arn:aws:iam::123456789012:role/AgentCoreRuntime

# AgentCore Gateway Configuration
AGENTCORE_GATEWAY_ENDPOINT=https://gateway.agentcore.ap-northeast-2.amazonaws.com
AGENTCORE_GATEWAY_API_KEY=your-gateway-api-key

# Bedrock Model Settings
BEDROCK_MODEL_ID=anthropic.claude-4-opus
BEDROCK_MAX_TOKENS=4096

# Observability (OpenTelemetry)
OTEL_EXPORTER_OTLP_ENDPOINT=http://localhost:4317
OTEL_SERVICE_NAME=aws-aiops-platform

# Development Settings
DEBUG=true
LOG_LEVEL=INFO
